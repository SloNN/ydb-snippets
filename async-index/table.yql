CREATE TABLE `async-index-demo` (
    a Int32 NOT NULL,
    b Text,
    PRIMARY KEY(a),
    INDEX ix_b GLOBAL ASYNC ON (b)
) WITH (
    AUTO_PARTITIONING_BY_SIZE = ENABLED,
    AUTO_PARTITIONING_BY_LOAD = ENABLED,
    AUTO_PARTITIONING_MIN_PARTITIONS_COUNT = 10,
    AUTO_PARTITIONING_MAX_PARTITIONS_COUNT = 20,
    PARTITION_AT_KEYS = (10000, 20000, 30000, 40000, 50000, 60000, 70000, 80000, 90000)
);
